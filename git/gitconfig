[alias]
        co = checkout
        st = status
        ci = commit
        l = log --graph --pretty=format':%C(yellow)%h%Cblue%d%Creset %s %C(white) %an, %ar%Creset'
        dc = diff --cached
        br = branch
[apply]
        whitespace = warn

[push]
        default = current
[color]
	ui = true

